<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Version</key>
	<integer>2</integer>
	<key>Extension Name</key>
	<string>Lingvo Translate</string>
	<key>Extension Identifier</key>
	<string>com.svyatov.popclip.extension.lingvo-translate</string>
	<key>Extension Description</key>
	<string>Open Lingvo Translate in a web browser with the selected text.</string>
	<key>Actions</key>
	<array>
		<dict>
			<key>URL</key>
			<string>http://lingvopro.abbyyonline.com/ru/Translate/en-ru/{popclip text}</string>
			<key>Title</key>
			<string>Translate</string>
			<key>Regular Expression</key>
			<string>(?s)^[a-zA-Z0-9.,;!?"'\s]{1,1000}$</string>
		</dict>
	</array>
</dict>
</plist>
